    <script>
      var styleJson = 'https://api.maptiler.com/maps/7afb9362-7272-4100-bae5-207038cc996b/style.json?key=eyg8faNmRC0xzoZf30cS';
      var map = new ol.Map({
        target: 'map',
        view: new ol.View({
          constrainResolution: true,
          center: ol.proj.fromLonLat([20.21532, 41.97217]),
          zoom: 4
        })
      });
      olms.apply(map, styleJson);
    </script>
